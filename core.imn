link l1 {
    nodes {n4 n2}
    bandwidth 0
}

link l2 {
    nodes {n3 n4}
    bandwidth 0
}

link l3 {
    ber 10
    nodes {n4 n1}
    bandwidth 0
}

link l4 {
    nodes {n1 n6}
    bandwidth 0
}

link l5 {
    nodes {n6 n5}
    bandwidth 0
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

